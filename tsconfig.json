{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "target": "es6",
        "moduleResolution": "node",
        "outDir": "dist",
        "watch": false
    },
    "files": [
        "src/main.ts",
        "src/models.ts",
        "src/messages.ts",
        "src/functions.ts",
        "src/config.json"
    ],
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
}
